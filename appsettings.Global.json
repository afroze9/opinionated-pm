{
  "ConsulKV": {
    "Url": "http://localhost:8500",
    "AddGlobalConfig": true,
    "GlobalConfigKey": "nexus-service/app-config"
  },
  "FrameworkSettings": {
    "Auth": {
      "Enable": true
    },
    "Swagger": {
      "Enable": false
    },
    "Telemetry": {
      "Enable": true
    },
    "Management": {
      "Enable": true
    },
    "Discovery": {
      "Enable": true
    },
    "Logging": {
      "Enable": true
    }
  }
}