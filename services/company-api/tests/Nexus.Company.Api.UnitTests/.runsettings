<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura</Format>
          <ExcludeByFile>**/*Migrations/*.cs</ExcludeByFile>
          <CodeCoverage>
            <ModulePaths>
              <Include>
                <ModulePath>Nexus.Company.Api</ModulePath>
              </Include>
              <Include>
                <ModulePath>.*tests.dll</ModulePath>
              </Include>
            </ModulePaths>
          </CodeCoverage>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
