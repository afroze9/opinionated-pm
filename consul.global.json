{
  "AllowedHosts": "*",
  "Auth0Settings": {
    "Authority": "https://teamly.us.auth0.com",
    "Audience": "projectmanagement"
  },
  "SerilogSettings": {
    "ElasticSearchSettings": {
      "Uri": "https://localhost:9200",
      "Username": "elastic",
      "Password": "dev123"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "TelemetrySettings": {
    "Endpoint": "http://localhost:4317",
    "EnableAlwaysOnSampler": false,
    "SampleProbability": 0.2,
    "EnableConsoleExporter": false
  },
  "management": {
    "endpoints": {
      "health": {
        "showdetails": "always"
      },
      "enabled": true,
      "actuator": {
        "exposure": {
          "include": [
            "*"
          ]
        }
      }
    }
  }
}